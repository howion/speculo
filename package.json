        "eslint-plugin-react": "^7.26.1",
        "next-plugin-preact": "^3.0.6",
        "next-transpile-modules": "^8.0.0",
        "sass": "^1.42.1",
        "stylelint": "^13.13.1",
        "stylelint-config-recess-order": "^2.5.0",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-formatter-pretty": "^3.0.0",
        "webpack": "^5.57.1"
